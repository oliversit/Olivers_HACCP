		    </div>
		
		
          <form class="needs-validation" action="/data" method="post">
			 <input id="Form_Name" name="Form_Name" type="hidden" value="Damaged_Discarded">
<%- include('../../Partials/Global_Variables') -%>
			  <input id="Form_Title" name="Form_Title" type="hidden" value="Forms">

		  
		 <div class="row">
              <div class="col-md-6 mb-3">
                <label for="Date">Date</label>
                <input type="Date" class="form-control" id="Date"  name="Date" >
                <div class="invalid-feedback">
                  Valid Date is .
                </div>
              </div>
					   
					   
              <div class="col-md-6 mb-3">
				  				                  <label for="country">Store Location</label>
                <select class="custom-select d-block w-100" id="country" name="FormStore" required>
                 <option value="<%=userStore %>"><%=userStore %></option>
                  <option value="Cotati">Cotati</option>
					<option value="Montecito">Montecito</option>
					<option value="Stony Point">Stony Point</option>
					<option value="Windsor">Windsor</option>
                </select>
                <div class="invalid-feedback">
                  Please select a valid Store.
          
              </div>
              </div>
					   
            </div>
			  
			  
			 
<!-------Item to be Disposed-------------------------------------------------------------------------------------------------------->
            <div class="row align-items-end">
              <div class="col-md-12 mb-3">
